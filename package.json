{
  "name": "blogger-tracker",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "electron:dev": "VITE_DEV_SERVER_URL=http://localhost:5173 electron dist-electron/main.js",
    "build": "vite build && electron-builder",
    "build:mac": "vite build && electron-builder --mac",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.77.0",
    "antd": "^5.27.4",
    "better-sqlite3": "^12.4.1",
    "dayjs": "^1.11.18",
    "playwright": "^1.56.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "electron": "^32.3.3",
    "electron-builder": "^25.1.8",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "vercel": "^48.7.0",
    "vite": "^7.1.7",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.0"
  },
  "build": {
    "appId": "com.blogger.tracker",
    "productName": "多项目博主管理系统",
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "build/icon.png"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ]
  }
}
